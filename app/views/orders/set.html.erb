<%= render partial: 'order_header', locals: { title: "Step 3: Select Set" , progress: 60 } %>

<%= bootstrap_form_for work_order, url: wizard_path, method: :put  do |f| %>

  <%= f.fields_for :item, item do |item_fields| %>

    <% get_all_sets.each do |p| %>
      <ul>
        <%= item_fields.radio_button "set_id", p[:id] %>
        <%= f.label p[:id] %>
        <%= f.label p[:name] %>
      </ul>
    <% end %>

  <% end %>


  <%= render 'buttons', work_order: work_order, f: f %>

<% end %>
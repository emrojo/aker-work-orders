<%= render partial: 'order_header', locals: { title: "Step 5: Confirm Order" , progress: 84 } %>

<%= bootstrap_form_for work_order, url: wizard_path, method: :put  do |f| %>

  <%= f.hidden_field :id %>

  <h2>Work order <%= work_order.id %></h2>

  <%= render 'products' %>

  <%= render 'sets' %>

  <%= render 'proposals' %>

  <%= render 'buttons', work_order: work_order, f: f %>

<% end %>